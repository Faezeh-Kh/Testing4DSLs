			if (recovered == null) {
				recovered = entry_if0.getKey();
			}
			resourcePackages = packages;
			resources = new ArrayList<Resource>();
			resources.add(model);
			referenceSelection = new SpecificReferenceSelection(resourcePackages, model, "thenStatement", recovered);
		} else {
