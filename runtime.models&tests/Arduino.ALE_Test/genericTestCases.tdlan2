Package arduinoGenericTestCases {
	Import all from common ;
	Import all from arduinoSpecificTypes ;
	Import all from testConfiguration ;
	
	Module moduleNewState ( level = * ) ;
	OutputModule outputmoduleNewState (level = * ) ;
	Led ledNewState ( level = * ) ;
	InputModule inputmoduleNewState ( level = * ) ;
	PushButton pushbuttonNewState ( level = * ) ;
	
	Test Description arduinoGenericTest uses configuration genericConfiguration{
		tester.genericTestGate sends {pushbuttonNewState (_name = "my_button", level = "1"), 
										ledNewState(_name = "RED_LED", level = "0")} to arduino.genericMUTGate;
		tester.genericTestGate sends runModel to arduino.genericMUTGate;
		tester.genericTestGate sends getModelState to arduino.genericMUTGate;
		//Assertion
		arduino.genericMUTGate sends ledNewState(_name = "RED_LED", 
													level = "1") to tester.genericTestGate;
		set verdict to PASS;
	}
}