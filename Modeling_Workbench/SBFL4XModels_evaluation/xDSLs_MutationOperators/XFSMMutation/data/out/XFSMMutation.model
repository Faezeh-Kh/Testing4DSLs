<?xml version="1.0" encoding="ASCII"?>
<mutatorenvironment:MutatorEnvironment xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:mutatorenvironment="http://mutatorenvironment/1.0">
  <definition xsi:type="mutatorenvironment:Program" metamodel="/XFSMMutation/data/model/k3fsm.ecore" output="data/out/" exhaustive="true">
    <source path="data/model/"/>
  </definition>
  <blocks name="cis" description="Changes the initial state to a different one">
    <commands xsi:type="mutatorenvironment:SelectObjectMutator" name="sm">
      <object xsi:type="mutatorenvironment:RandomTypeSelection">
        <type href="/XFSMMutation/data/model/k3fsm.ecore#//FSM"/>
      </object>
    </commands>
    <commands xsi:type="mutatorenvironment:SelectObjectMutator" name="cis">
      <container xsi:type="mutatorenvironment:SpecificObjectSelection" objSel="//@blocks.0/@commands.0">
        <refType href="/XFSMMutation/data/model/k3fsm.ecore#//FSM/initialState"/>
      </container>
      <object xsi:type="mutatorenvironment:RandomTypeSelection">
        <type href="/XFSMMutation/data/model/k3fsm.ecore#//State"/>
      </object>
    </commands>
    <commands xsi:type="mutatorenvironment:SelectObjectMutator" name="nis">
      <object xsi:type="mutatorenvironment:RandomTypeSelection">
        <type href="/XFSMMutation/data/model/k3fsm.ecore#//State"/>
        <expression>
          <first xsi:type="mutatorenvironment:ReferenceEvaluation" operator="different">
            <value xsi:type="mutatorenvironment:SpecificObjectSelection" objSel="//@blocks.0/@commands.1"/>
          </first>
        </expression>
      </object>
    </commands>
    <commands xsi:type="mutatorenvironment:ModifyInformationMutator">
      <object xsi:type="mutatorenvironment:SpecificObjectSelection" objSel="//@blocks.0/@commands.0"/>
      <references xsi:type="mutatorenvironment:ReferenceInit">
        <reference href="/XFSMMutation/data/model/k3fsm.ecore#//FSM/initialState"/>
        <object xsi:type="mutatorenvironment:SpecificObjectSelection" objSel="//@blocks.0/@commands.2"/>
      </references>
    </commands>
  </blocks>
  <blocks name="cst" description="Changes the source state of a transition">
    <commands xsi:type="mutatorenvironment:SelectObjectMutator" name="transition">
      <object xsi:type="mutatorenvironment:RandomTypeSelection">
        <type href="/XFSMMutation/data/model/k3fsm.ecore#//Transition"/>
      </object>
    </commands>
    <commands xsi:type="mutatorenvironment:SelectObjectMutator" name="state">
      <object xsi:type="mutatorenvironment:RandomTypeSelection">
        <type href="/XFSMMutation/data/model/k3fsm.ecore#//State"/>
        <expression>
          <first xsi:type="mutatorenvironment:ReferenceEvaluation" operator="different">
            <value xsi:type="mutatorenvironment:SpecificObjectSelection" objSel="//@blocks.1/@commands.0">
              <refType href="/XFSMMutation/data/model/k3fsm.ecore#//Transition/source"/>
            </value>
          </first>
        </expression>
      </object>
    </commands>
    <commands xsi:type="mutatorenvironment:ModifyInformationMutator">
      <object xsi:type="mutatorenvironment:SpecificObjectSelection" objSel="//@blocks.1/@commands.0"/>
      <references xsi:type="mutatorenvironment:ReferenceInit">
        <reference href="/XFSMMutation/data/model/k3fsm.ecore#//Transition/source"/>
        <object xsi:type="mutatorenvironment:SpecificObjectSelection" objSel="//@blocks.1/@commands.1"/>
      </references>
    </commands>
  </blocks>
  <blocks name="ctt" description="Changes the target state of a transition">
    <commands xsi:type="mutatorenvironment:SelectObjectMutator" name="transition">
      <object xsi:type="mutatorenvironment:RandomTypeSelection">
        <type href="/XFSMMutation/data/model/k3fsm.ecore#//Transition"/>
      </object>
    </commands>
    <commands xsi:type="mutatorenvironment:SelectObjectMutator" name="state">
      <object xsi:type="mutatorenvironment:RandomTypeSelection">
        <type href="/XFSMMutation/data/model/k3fsm.ecore#//State"/>
        <expression>
          <first xsi:type="mutatorenvironment:ReferenceEvaluation" operator="different">
            <value xsi:type="mutatorenvironment:SpecificObjectSelection" objSel="//@blocks.2/@commands.0">
              <refType href="/XFSMMutation/data/model/k3fsm.ecore#//Transition/target"/>
            </value>
          </first>
        </expression>
      </object>
    </commands>
    <commands xsi:type="mutatorenvironment:ModifyInformationMutator">
      <object xsi:type="mutatorenvironment:SpecificObjectSelection" objSel="//@blocks.2/@commands.0"/>
      <references xsi:type="mutatorenvironment:ReferenceInit">
        <reference href="/XFSMMutation/data/model/k3fsm.ecore#//Transition/target"/>
        <object xsi:type="mutatorenvironment:SpecificObjectSelection" objSel="//@blocks.2/@commands.1"/>
      </references>
    </commands>
  </blocks>
  <blocks name="mti" description="change input of a transition">
    <commands xsi:type="mutatorenvironment:ModifyInformationMutator">
      <object xsi:type="mutatorenvironment:RandomTypeSelection">
        <type href="/XFSMMutation/data/model/k3fsm.ecore#//Transition"/>
      </object>
      <attributes xsi:type="mutatorenvironment:AttributeScalar">
        <attribute href="/XFSMMutation/data/model/k3fsm.ecore#//Transition/input"/>
        <value xsi:type="mutatorenvironment:RandomStringType" min="1" max="4"/>
      </attributes>
    </commands>
  </blocks>
  <blocks name="mto" description="change output of a transition">
    <commands xsi:type="mutatorenvironment:ModifyInformationMutator">
      <object xsi:type="mutatorenvironment:RandomTypeSelection">
        <type href="/XFSMMutation/data/model/k3fsm.ecore#//Transition"/>
      </object>
      <attributes xsi:type="mutatorenvironment:AttributeScalar">
        <attribute href="/XFSMMutation/data/model/k3fsm.ecore#//Transition/output"/>
        <value xsi:type="mutatorenvironment:RandomStringType" min="1" max="4"/>
      </attributes>
    </commands>
  </blocks>
</mutatorenvironment:MutatorEnvironment>
