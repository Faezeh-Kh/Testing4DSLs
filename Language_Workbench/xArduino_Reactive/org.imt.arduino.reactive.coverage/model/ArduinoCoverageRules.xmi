<?xml version="1.0" encoding="ASCII"?>
<DSLSpecificCoverage:DomainSpecificCoverage
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:DSLSpecificCoverage="https://org.imt.tdl.coverage/DSLSpecificCoverage"
    name="ArduinoCoverage">
  <contexts>
    <metaclass
        href="../../org.imt.arduino.reactive.model/model/arduino.ecore#//Sketch"/>
    <rules xsi:type="DSLSpecificCoverage:ContainerCoverageInference"
        multiplicity="ONE">
      <condition
          xsi:type="DSLSpecificCoverage:ContainerInclusion">
        <whenContainedBy
            href="../../org.imt.arduino.reactive.model/model/arduino.ecore#//Project"/>
      </condition>
      <containementReference
          href="../../org.imt.arduino.reactive.model/model/arduino.ecore#//Project/sketches"/>
    </rules>
  </contexts>
  <contexts>
    <metaclass
        href="../../org.imt.arduino.reactive.model/model/arduino.ecore#//Block"/>
    <rules xsi:type="DSLSpecificCoverage:ContainerCoverageInference"
        multiplicity="ONE">
      <condition
          xsi:type="DSLSpecificCoverage:ContainerInclusion">
        <whenContainedBy
            href="../../org.imt.arduino.reactive.model/model/arduino.ecore#//Sketch"/>
      </condition>
      <containementReference
          href="../../org.imt.arduino.reactive.model/model/arduino.ecore#//Sketch/block"/>
    </rules>
  </contexts>
  <contexts>
    <metaclass
        href="../../org.imt.arduino.reactive.model/model/arduino.ecore#//Instruction"/>
    <rules xsi:type="DSLSpecificCoverage:ContainerCoverageInference"
        multiplicity="ONE">
      <condition
          xsi:type="DSLSpecificCoverage:ContainerInclusion">
        <whenContainedBy
            href="../../org.imt.arduino.reactive.model/model/arduino.ecore#//Block"/>
      </condition>
      <containementReference
          href="../../org.imt.arduino.reactive.model/model/arduino.ecore#//Block/instructions"/>
    </rules>
  </contexts>
  <contexts>
    <metaclass
        href="../../org.imt.arduino.reactive.model/model/arduino.ecore#//Board"/>
    <rules xsi:type="DSLSpecificCoverage:CoverageIgnorance"
        ignoreSubclasses="true"/>
  </contexts>
  <contexts>
    <metaclass
        href="../../org.imt.arduino.reactive.model/model/arduino.ecore#//Pin"/>
    <rules xsi:type="DSLSpecificCoverage:CoverageIgnorance"
        ignoreSubclasses="true"/>
  </contexts>
  <contexts>
    <metaclass
        href="../../org.imt.arduino.reactive.model/model/arduino.ecore#//Module"/>
    <rules xsi:type="DSLSpecificCoverage:CoverageIgnorance"
        ignoreSubclasses="true"/>
  </contexts>
</DSLSpecificCoverage:DomainSpecificCoverage>
