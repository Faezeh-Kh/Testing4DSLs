<?xml version="1.0" encoding="ASCII"?>
<DSLSpecificCoverage:DomainSpecificCoverage
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:DSLSpecificCoverage="https://org.imt.tdl.coverage/DSLSpecificCoverage"
    name="MiniJavaCoverage">
  <contexts>
    <metaclass
        href="../../org.imt.xminijava.model/model/XMiniJava.ecore#//ForStatement"/>
    <rules xsi:type="DSLSpecificCoverage:CoverageIgnorance"
        ignoreSubclasses="true"/>
  </contexts>
  <contexts>
    <metaclass
        href="../../org.imt.xminijava.model/model/XMiniJava.ecore#//WhileStatement"/>
    <rules xsi:type="DSLSpecificCoverage:CoverageIgnorance"
        ignoreSubclasses="true"/>
  </contexts>
  <contexts>
    <metaclass
        href="../../org.imt.xminijava.model/model/XMiniJava.ecore#//IfStatement"/>
    <rules xsi:type="DSLSpecificCoverage:CoverageIgnorance"
        ignoreSubclasses="true"/>
  </contexts>
  <contexts>
    <metaclass
        href="../../org.imt.xminijava.model/model/XMiniJava.ecore#//Method"/>
    <rules xsi:type="DSLSpecificCoverage:CoverageIgnorance"
        ignoreSubclasses="true"/>
  </contexts>
  <contexts>
    <metaclass
        href="../../org.imt.xminijava.model/model/XMiniJava.ecore#//Program"/>
    <rules xsi:type="DSLSpecificCoverage:CoverageIgnorance"
        ignoreSubclasses="true"/>
  </contexts>
  <contexts>
    <metaclass
        href="../../org.imt.xminijava.model/model/XMiniJava.ecore#//MethodCall"/>
    <rules xsi:type="DSLSpecificCoverage:CoverageIgnorance"
        ignoreSubclasses="true">
      <condition
          xsi:type="DSLSpecificCoverage:ContainerExclusion">
        <whenNotContainedBy
            href="../../org.imt.xminijava.model/model/XMiniJava.ecore#//Block"/>
      </condition>
    </rules>
  </contexts>
  <contexts>
    <metaclass
        href="../../org.imt.xminijava.model/model/XMiniJava.ecore#//Member"/>
    <rules xsi:type="DSLSpecificCoverage:ContainerCoverageInference"
        multiplicity="ONE">
      <condition
          xsi:type="DSLSpecificCoverage:ContainerInclusion">
        <whenContainedBy
            href="../../org.imt.xminijava.model/model/XMiniJava.ecore#//TypeDeclaration"/>
      </condition>
      <containementReference
          href="../../org.imt.xminijava.model/model/XMiniJava.ecore#//TypeDeclaration/members"/>
    </rules>
  </contexts>
</DSLSpecificCoverage:DomainSpecificCoverage>
