<?xml version="1.0" encoding="ASCII"?>
<DSLSpecificCoverage:DomainSpecificCoverage
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:DSLSpecificCoverage="https://org.imt.tdl.coverage/DSLSpecificCoverage"
    name="MiniJavaCoverage">
  <contexts>
    <metaclass
        href="../../org.imt.xminijava.model/model/XMiniJava.ecore#//Program"/>
    <rules xsi:type="DSLSpecificCoverage:Ignore"
        ignoreIfSubtyped="true"/>
  </contexts>
  <contexts>
    <metaclass
        href="../../org.imt.xminijava.model/model/XMiniJava.ecore#//Method"/>
    <rules xsi:type="DSLSpecificCoverage:Ignore"
        ignoreIfSubtyped="true"/>
  </contexts>
  <contexts>
    <metaclass
        href="../../org.imt.xminijava.model/model/XMiniJava.ecore#//MethodCall"/>
    <rules xsi:type="DSLSpecificCoverage:IgnoreIfContained"
        condition="EXCLUSION">
      <containerType
          href="../../org.imt.xminijava.model/model/XMiniJava.ecore#//Block"/>
    </rules>
  </contexts>
  <contexts>
    <metaclass
        href="../../org.imt.xminijava.model/model/XMiniJava.ecore#//Class"/>
    <rules xsi:type="DSLSpecificCoverage:CoverageByContainment"
        multiplicity="ONE">
      <containmentReference
          href="../../org.imt.xminijava.model/model/XMiniJava.ecore#//TypeDeclaration/members"/>
    </rules>
  </contexts>
</DSLSpecificCoverage:DomainSpecificCoverage>
